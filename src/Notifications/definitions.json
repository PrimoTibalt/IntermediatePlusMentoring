{
	"vhosts": [
		{
			"name": "/"
		}
	],
	"users": [
		{
			"name": "user",
			"password": "pass",
			"hashing_algorithm": "rabbit_password_hashing_sha256",
			"tags": "administrator"
		}
	],
	"queues": [
		{
			"name": "booking",
			"vhost": "/",
			"durable": true,
			"auto_delete": false,
			"arguments": {
				"x-queue-type": "classic"
			}
		},
		{
			"name": "paymentComplete",
			"vhost": "/",
			"durable": true,
			"auto_delete": false,
			"arguments": {
				"x-queue-type": "classic"
			}
		},
		{
			"name": "paymentFail",
			"vhost": "/",
			"durable": true,
			"auto_delete": false,
			"arguments": {
				"x-queue-type": "classic"
			}
		}
	],
	"exchanges": [
		{
			"name": "BookingExchange",
			"vhost": "/",
			"type": "direct",
			"durable": true,
			"auto_delete": false,
			"internal": false,
			"arguments": {}
		},
		{
			"name": "PaymentCompleteExchange",
			"vhost": "/",
			"type": "direct",
			"durable": true,
			"auto_delete": false,
			"internal": false,
			"arguments": {}
		},
		{
			"name": "PaymentFailExchange",
			"vhost": "/",
