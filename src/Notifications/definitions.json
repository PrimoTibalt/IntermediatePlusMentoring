	"bindings": [
		{
			"source": "BookingExchange",
			"vhost": "/",
			"destination": "booking",
			"destination_type": "queue",
			"routing_key": "booking",
			"arguments": {}
		},
		{
			"source": "PaymentCompleteExchange",
			"vhost": "/",
			"destination": "paymentComplete",
			"destination_type": "queue",
			"routing_key": "paymentComplete",
			"arguments": {}
		},
		{
			"source": "PaymentFailExchange",
			"vhost": "/",
			"destination": "paymentFail",
			"destination_type": "queue",
			"routing_key": "paymentFail",
			"arguments": {}
		}
	],
	"permissions": [
		{
			"user": "user",
			"vhost": "/",
			"configure": ".*",
			"write": ".*",
			"read": ".*"
